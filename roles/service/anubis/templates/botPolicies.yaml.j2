bots:
{% if anubis_allowed_ip_ranges|length > 0 %}
  - name: allowed-ip-ranges
    action: ALLOW
    remote_addresses:
{% for ip_range in anubis_allowed_ip_ranges %}
      - {{ ip_range }}
{% endfor %}
{% endif %}
  - name: allow-dhall-files
    action: ALLOW
    path_regex: "^.*dhall$"
  - import: (data)/clients/git.yaml
  - import: (data)/meta/default-config.yaml
  - name: challenge-mozilla
    user_agent_regex: Mozilla
    action: CHALLENGE